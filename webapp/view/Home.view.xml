<mvc:View
	controllerName="sap.ui.demo.basicTemplate.controller.Home"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="{i18n>title}">
		<content>
            <List headerText="Employees from Northwind" items="{data>/Employees}">
				<StandardListItem title="{data>FirstName}"/>
			</List>
        </content>
	</Page>
</mvc:View>